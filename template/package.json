{
  "name": "vue-plugin",
  "version": "0.0.1",
  "description": "A Vue.js plugin",
  "author": "Eduardo San Martin Morote <posva13@gmail.com>",
  "main": "dist/vue-plugin.js",
  "scripts": {
    "build": "node build/build.js",
    "lint": "eslint --ext js --ext jsx --ext vue src test/**/*.spec.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test karma start test/unit/karma.conf.js --single-run",
    "dev": "webpack-dashboard -- webpack-dev-server --config build/webpack.test.config.js --open",
    "dev:coverage": "cross-env NODE_ENV=test karma start test/unit/karma.conf.js"
  },
  "dependencies": {
    "vue": "^2.1.6"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "buble": "^0.15.1",
    "chai": "^3.5.0",
    "clean-css": "^3.4.23",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-vue": "^1.0.0",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.24.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "mocha-css": "^1.0.1",
    "object-assign": "^4.1.0",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-vue": "^2.2.14",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.6",
    "webpack": "beta",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.1.2"
  },
